#include <iostream>
#include <algorithm>
#include <string>

using namespace std;




int main() {
	string str;
	cin >> str;

	int len = str.size();
	int i = 0;
	int j = len - 1;
	int index;
	int flag = 0;

	while (i < j) {
		if (str[i] != str[j]) {
			flag++;
			if (flag == 1) {
				index = i;
				i++;
			}else if (flag == 2) {
				i = index;
				j = len - i - 2;
			}
			else {
				break;
			}
			
			continue;
		}
		i++;
		j--;
	}
	if (flag == 1) {
		str.erase(str.begin() + index);
	}
	else if (flag == 2) {
		str.erase(str.end()-index-1);
	}
	else if(flag >2){
		str = "false";
	}
	cout << str << endl;
	
	return 0;
}
