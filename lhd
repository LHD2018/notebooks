#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

struct Data
{

	vector<int> people;
	vector<int> actors;
};

void findResult(Data& data) {
	int index;
	bool flag;
	for (auto i : data.people) {
		index = 1;
		flag = false;
		for(auto j : data.actors){
			if (i <= j) {
				cout << index << " ";
				flag = true;
				break;
			}
			index++;
		}
		if (!flag)
		{
			cout << -1 << " ";
		}
	}
	cout << endl;
}

int main() {
	int nums;
	cin >> nums;

	vector<Data> datas;
	Data data;
	int temp;
	int people_num;
	int actor_num;
	for (int i = 0; i < nums; i++)
	{
		cin >> people_num >> actor_num;
		
		for (int j = 0; j < people_num; j++) {
			cin >> temp;
			data.people.push_back(temp);
		}
		for (int k = 0; k< actor_num; k++) {
			cin >> temp;
			data.actors.push_back(temp);
		}
		datas.push_back(data);

	}

	for (auto k : datas) {
		findResult(k);
	}
	
	return 0;
}
